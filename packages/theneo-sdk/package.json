{
  "name": "@theneo/sdk",
  "version": "0.0.1",
  "description": "Theneo SDK",
  "engines": {
    "node": ">=16.0"
  },
  "keywords": [
    "theneo",
    "api",
    "documentation",
    "sdk",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/Theneo-Inc/theneo-cli/issues"
  },
  "homepage": "https://github.com/Theneo-Inc/theneo-cli#readme",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*.mjs": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.mjs"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.mjs"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src/  --fix"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "form-data": "^4.0.0",
    "tsup": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.17.17",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "tsconfig": "*",
    "typescript": "^5.2.2"
  }
}
