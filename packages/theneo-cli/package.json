{
  "name": "@theneo/cli",
  "version": "0.3.1",
  "description": "Theneo CLI",
  "keywords": [
    "theneo",
    "api",
    "documentation",
    "cli",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/Theneo-Inc/theneo-tools/tree/main/packages/theneo-cli#readme",
  "bugs": {
    "url": "https://github.com/Theneo-Inc/theneo-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Theneo-Inc/theneo-tools.git"
  },
  "license": "MIT",
  "author": {
    "name": "theneo",
    "url": "https://github.com/Theneo-Inc"
  },
  "bin": {
    "theneo": "./bin/index.js"
  },
  "files": [
    "dist/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "npm run build -- --watch",
    "ci": "npm run build",
    "clean": "rm -rf ./dist/",
    "install-locally": "npm run lint && npm run build && npm i -g ../theneo-cli",
    "lint": "eslint ./src/  --fix",
    "release": "npm run ci && changeset publish",
    "start": "npm run build && TS_NODE_FILES=true ts-node -T ./src/bin.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.1.2",
    "@theneo/sdk": "*",
    "axios": "^1.5.1",
    "chalk": "^5.3.0",
    "cli-table": "^0.3.11",
    "commander": "^11.0.0",
    "execa": "^8.0.1",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.0",
    "nanospinner": "^1.1.0",
    "open": "^9.1.0",
    "toml": "^3.0.0",
    "yaml-lint": "^1.7.0"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.2",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^18.17.17",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "type-fest": "^4.3.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0"
  }
}
