{
  "name": "@theneo/sdk",
  "version": "0.13.0",
  "description": "Theneo SDK",
  "keywords": [
    "theneo",
    "api",
    "documentation",
    "sdk",
    "typescript"
  ],
  "homepage": "https://github.com/Theneo-Inc/theneo-tools/tree/main/packages/theneo-sdk#readme",
  "bugs": {
    "url": "https://github.com/Theneo-Inc/theneo-tools/issues"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "pnpm run build -- --watch",
    "lint": "eslint ./src/  --fix",
    "madge": "madge -c src/index.ts --ts-config tsconfig.build.json"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.24.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
