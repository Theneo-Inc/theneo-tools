{
  "name": "theneo-tools",
  "version": "1.0.0",
  "description": "## Theneo tools",
  "packageManager": "npm@9.6.7",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {

  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "tsconfig": "*",
    "turbo": "latest",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1"
  },
  "scripts": {
    "prepare": "husky install",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "install:cli": "turbo build lint && npm i -g ./packages/theneo-cli"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix"
  }
}
